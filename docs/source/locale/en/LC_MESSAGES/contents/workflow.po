msgstr ""

#: ../../contents/workflow.rst:80
msgid ""
"$pattern = \"*p4\"\n"
"java -jar sfinder.jar util seq -M pass -hh no -p $pattern -e '^.{0,1}?S' "
"| Out-File -Encoding default input/patterns.txt\n"
"java -jar sfinder.jar percent -t v115@9gB8DeG8CeH8BeG8CeD8JeAgH -fc 0 -td"
" 1"
